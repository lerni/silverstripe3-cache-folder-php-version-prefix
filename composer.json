{
    "name": "lerni/silverstripe3-cachefolder-phpversion-prefix",
    "description": "A patch for SilverStripe to prefix cache-folder with php version - prevent 503 on version change",
    "type": "silverstripe-patch",
    "homepage": "http://github.com/lerni/silverstripe3-cachefolder-phpversion-prefix",
    "keywords": ["silverstripe", "cache", "patch"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Lukas Erni",
            "email": "le@kraftausdruck.ch"
        }
    ],
    "support": {
        "issues": "http://github.com/lerni/silverstripe3-cachefolder-phpversion-prefix"
    },
    "require": {
        "silverstripe/framework": ">=3.6.0 <3.7.0",
        "cweagans/composer-patches": "^1.6.4"
    },
    "extra": {
        "patches-file": "composer.patches.json",
        "enable-patching": true
    }
}